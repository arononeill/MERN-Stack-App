    "express": "^4.18.2",
    "font-awesome": "^4.7.0",
    "jsonwebtoken": "^8.5.1",
    "mdbreact": "^4.25.6",
    "mongoose": "^5.9.9",
    "react-bootstrap": "^1.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.3"
  }
}
